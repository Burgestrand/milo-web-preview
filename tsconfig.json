{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@config/*": [
        "src/lib/config/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@stores/*": [
        "src/lib/stores/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}